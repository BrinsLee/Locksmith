<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FontTextView">
        <attr name="font_type" format="enum">
            <enum name="DIN-Bold" value="1" />
            <enum name="DIN-Medium" value="2" />
            <enum name="DIN-Regular" value="3" />
            <enum name="LilyScriptOne-Regular" value="4" />
        </attr>
        <attr name="start_color" format="color" />
        <attr name="end_color" format="color" />
        <attr name="stroke" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RoundConstraintLayout">
        <attr name="round_corner" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PxBlurringView">
        <attr name="blur_radius" format="integer" />
        <attr name="sample_factor" format="integer" />
        <attr name="overlay_color" format="color" />
    </declare-styleable>
</resources>